<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE configuration SYSTEM "../../../../dtds/ActionElementConfiguration.dtd">
<configuration class="com.audium.server.action.database.DatabaseAction" serial="0000">
<setting name="type">insert</setting>
<setting name="jndiName">cvp_pcs</setting>
<setting name="query"><![CDATA[INSERT INTO postcallsurvey 
(DateTime,
ANI,
Q1,
Q2,
Q3,
Telp,
AgentID,
SkillID,
TeamID)
VALUES 
('{0}-{1}-{2} {3}:{4}:00',
'{5}',
'{6}',
'{7}',
'{8}',
'{9}',
'{10}',
'{11}',
'{12}')]]></setting>
<setting name="enableXmlResultSet">true</setting>
<setting name="maxRowsInXML">1</setting>
<substitute index="0">
<general_date_time field="year" type="current"/>
</substitute>
<substitute index="1">
<general_date_time field="month" type="current"/>
</substitute>
<substitute index="2">
<general_date_time field="day_of_month" type="current"/>
</substitute>
<substitute index="3">
<general_date_time field="hour_of_day" type="current"/>
</substitute>
<substitute index="4">
<general_date_time field="minute" type="current"/>
</substitute>
<substitute index="5">
<call_data type="ani"/>
</substitute>
<substitute index="6">
<local_variable name="Q1"/>
</substitute>
<substitute index="7">
<local_variable name="Q2"/>
</substitute>
<substitute index="8">
<local_variable name="Q3"/>
</substitute>
<substitute index="9">
<local_variable name="PhoneNumber"/>
</substitute>
<substitute index="10">
<local_variable name="AgentID"/>
</substitute>
<substitute index="11">
<local_variable name="SkillID"/>
</substitute>
<substitute index="12">
<local_variable name="TeamID"/>
</substitute>
</configuration>
